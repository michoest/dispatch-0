# Server Configuration
PORT=3100

# OpenAI API Key (required for LLM routing)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Dispatcher API Key (used by Siri to authenticate)
DISPATCHER_API_KEY=your-secure-dispatcher-api-key-here

# OpenAI Model (optional, defaults to gpt-4-turbo-preview)
LLM_MODEL=gpt-4-turbo-preview

# Confidence Threshold (optional, defaults to 0.7)
CONFIDENCE_THRESHOLD=0.7

# Health Check Interval in ms (optional, defaults to 60000)
HEALTH_CHECK_INTERVAL=60000

# Database Path (optional, defaults to ./db.json)
DB_PATH=./db.json
